pipeline {
    agent any
    stages {
        stage("Build") {
            steps {
                echo "hello jancok"
            }
        }
        stage("Test") {
            steps {
                echo "hello jancok"
            }
        }
        stage("Deploy") {
            steps {
                echo "hello jancok"
            }
        }
    }
    post {
        success {
            echo "success Deploy !"
        }
        failure {
            echo "Failure Deploy !"
        }
    }
}